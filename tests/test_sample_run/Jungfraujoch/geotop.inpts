HeaderDateDDMMYYYYhhmmMeteo  = "Date"
HeaderAirTemp                = "AirT"
HeaderWindVelocity           = "WindS"
HeaderWindDirection          = "WindDir"
HeaderRH                     = "RelHum"
HeaderSWglobal               = "SWglobal"
HeaderCloudSWTransmissivity  = "Cloud"
