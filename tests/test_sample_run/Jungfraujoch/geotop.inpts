HeaderCoordinatePointX 		 = "X"
HeaderCoordinatePointY 		 = "Y"
HeaderPointElevation         = "ele"
HeaderPointSlope             = "slp"
HeaderPointAspect            = "asp"
HeaderPointSkyViewFactor     = "sky"
HeaderPointHorizon 			 = "hor"

SuccessfulRunFile 	   = "_SUCCESSFUL_RUN"
